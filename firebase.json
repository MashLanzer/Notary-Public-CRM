{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/booking",
        "destination": "/booking.html"
      },
      {
        "source": "/landing",
        "destination": "/landing.html"
      },
      {
        "source": "/status",
        "destination": "/status.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firebase.rules"
  }
}